	<div class="row masonry-wrap">
		<div class="masonry">
			<c:forEach items="${MyJob}" var="job">
				<article id="post_13"
					class="masonry__brick entry format-standard post-15 post type-post status-publish has-post-thumbnail hentry category-health"
					data-aos="fade-up">
				<div class="entry__thumb">
					<img
						alt="https://i.pinimg.com/236x/c7/f6/e4/c7f6e4692bca4de126a739d873b18847--himuoto-umaru-chan-umaru-doma.jpg"
						src="${job.img}">
				</div>
				<div class="entry__text">
					<div class="entry__header">

						<h1 class="entry__title">Title: ${job.title}</h1>
						<h5>Id Job: ${job.id}</h5>
						<h5>Id People Create Job: ${job.idLeader}</h5>
					</div>
					<div class="entry__excerpt">
						<p>Description: ${job.des}</p>
						<p>Time Create: ${job.timeCreate}</p>
						<p>Time Complete: ${job.timeEnd}</p>
						<h4>Status: ${job.status}</h4>
					</div>
				</div>
				</article>
				<form action="/submitjob">
					<input type="hidden" name="idJob" value="${job.id}"> <input
						type="submit" value="Submit This Job">
				</form>
			</c:forEach>
		</div>
	</div>


